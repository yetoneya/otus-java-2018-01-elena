<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.text.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane id="withdrawMoneyPane"  prefHeight="500" prefWidth="600" alignment="CENTER" hgap="10" vgap="10"  xmlns:fx="http://javafx.com/fxml/1" fx:controller="ru.otus.elena.atmmashine.view.FXMLWithdrawMoneyController">
    <TextField fx:id="sumOfMoney" prefHeight="50.0" prefWidth="500.0" text="100"  GridPane.columnIndex="0" GridPane.rowIndex="0"/>
    <Button fx:id="withdrawMoneyButton" minHeight="50.0" minWidth="500.0" text="Снять" onAction="#handleWithdrawMoneyButton" GridPane.columnIndex="0" GridPane.rowIndex="1"/>
    <Label fx:id="messageLabel" minHeight="50.0" prefWidth="500.0"  text=""  GridPane.columnIndex="0" GridPane.rowIndex="2"/>
</GridPane>
